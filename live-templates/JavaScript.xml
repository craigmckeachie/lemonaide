<templateSet group="JavaScript">
  <template name="iife" value="(function($ARGS$){&#10;    &quot;use strict&quot;;&#10;&#10;    $END$&#10;    &#10;})($ARGS$);" description="Creates Immediately Invoked Function Expression" toReformat="true" toShortenFQNames="true">
    <variable name="ARGS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="false" />
      <option name="JS_EXPRESSION" value="false" />
      <option name="JSX_HTML" value="false" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="func" value="function($PARAM$) {&#10;  $END$&#10;}" shortcut="TAB" description="Inserts function expression" toReformat="true" toShortenFQNames="true">
    <variable name="PARAM" expression="" defaultValue="&quot;&quot;" alwaysStopAt="true" />
    <context />
  </template>
  <template name="fn" value="function $FN$($PARAMS$) {&#10;    $END$&#10;}" description="Creates Named Function Statement or Declaration" toReformat="true" toShortenFQNames="true">
    <variable name="FN" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="PARAMS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="log" value="console.log($params$);&#10;$end$" description="console.log" toReformat="false" toShortenFQNames="true">
    <variable name="params" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="end" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="false" />
      <option name="JS_EXPRESSION" value="false" />
      <option name="JSX_HTML" value="false" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>